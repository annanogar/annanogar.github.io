{
  "name": "design-system",
  "version": "2.0.0",
  "description": "Fabrique Design System",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/fabrique/design-system.git"
  },
  "private": true,
  "main": "./sonic/index.js",
  "type": "module",
  "bin": {
    "sonic": "./sonic/index.js"
  },
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "postinstall": "node sonic --postinstall",
    "start": "node sonic",
    "dev": "node sonic",
    "build": "node sonic build && yarn prettier . 'build/**/*.html' --write --log-level error",
    "deploy": "node sonic deploy"
  },
  "devDependencies": {
    "@eklingen/esbuild-browserslist": "1.0.1",
    "@eklingen/file-hash-cache": "1.1.4",
    "@eklingen/glob-importers": "2.1.1",
    "@eklingen/nunjucks-dependencies": "1.0.0",
    "@parcel/watcher": "2.5.1",
    "autoprefixer": "10.4.21",
    "browserslist": "4.25.1",
    "esbuild": "0.25.6",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "globals": "16.3.0",
    "nunjucks": "3.2.4",
    "picomatch": "4.0.2",
    "postcss": "8.5.6",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "prettier-plugin-jinja-template": "2.1.0",
    "sass-embedded": "1.89.2",
    "sharp": "0.34.3",
    "stylelint": "16.21.1",
    "stylelint-config-recommended-scss": "15.0.1",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "7.0.0",
    "stylelint-scss": "6.12.1",
    "svgo": "4.0.0",
    "tinyglobby": "0.2.14",
    "yazl": "3.3.1"
  },
  "optionalDevDependencies": {
    "dotenv": "16.4.5"
  },
  "dependencies": {
    "focus-trap": "7.6.5"
  },
  "resolutionExplanations": {
    "node-gyp": "linked to null because we don't need it - note: this breaks fs-events and @parcel/watcher (re-)compilation"
  },
  "resolutions": {
    "node-gyp": "link:./node_modules/.null"
  }
}
